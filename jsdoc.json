{
    "source": {
        "includePattern": ".+\\.tsx?$"
    },
    "plugins": [
        "plugins/markdown",
        "node_modules/jsdoc-babel"
    ],
    "babel": {
        "presets": [
            "@babel/preset-typescript",
            [
                "@babel/preset-env",
                {
                    "targets": {
                        "node": "current"
                    }
                }
            ]
        ]
    }
}
